    "/static/getDicByProdAndBiz": {
      "post": {
        "operationId": "getDicByProdAndBiz",
        "summary": "根据产品码(ProdCd)跟业务码(BizCd)获取二级目录bizcd或者交易类型(transcd)",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "ProdCd",
            "in": "body",
            "description": "如果BizCd等于空， 则找到ProdCd下的全部BizCd如果bizCd 不为空， 则查找ProdCd跟BizCd下的全部TransCd ",
            "schema": {
              "type": "object",
              "allOf": [
                {
                  "required": [
                    "ProdCd"
                  ],
                  "properties": {
                    "prodCd": {
                      "type": "string"
                    },
                    "bizCd": {
                      "type": "string"
                    }
                  }
                }
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "正确返回",
            "examples": {
              "application/json": {
                "items": [
                  {
                    "dicType": "BIZ_CD",
                    "dicCode": "0000000",
                    "dicName": "0000000_消费"
                  },
                  {
                    "dicType": "BIZ_CD",
                    "dicCode": "0000007",
                    "dicName": "0000007_微信支付"
                  },
                  {
                    "dicType": "BIZ_CD",
                    "dicCode": "0000008",
                    "dicName": "0000008_支付宝支付"
                  }
                ],
                "err": null
              }
            }
          },
          "400": {
            "description": "错误返回",
            "examples": {
              "application/json": {
                "desc": "必须传入ProdCd",
                "err": "InvalidParamError"
              }
            }
          }
        }
      }
    },
